<div class="col-sm">
	<h1>Detalhes do Exame</h1>

	<div class='col-sm'>
	<p>
		<b>Tipo do Exame: </b> <%= @exam.exam_type %> <br/>
		<b>Data do Exame:</b> <%= @exam.exam_date.strftime("%d/%m/%Y") %>
		<b>Data da Entrega:</b> <%= @exam.delivery_date.strftime("%d/%m/%Y") %><br/>
	</p>
	<div/>
	<%= link_to 'Voltar', patient_exams_path(@patient), class: "btn btn-primary" %>
	<%= link_to 'Editar', edit_patient_exam_path(@patient,@exam), class: "btn btn-primary" %>
</div>
