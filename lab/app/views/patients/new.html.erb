
<%= form_with scope: :patient, url: patients_path, local: true do |form| %>
    <p>
      <%= form.label :nome %><br>
      <%= form.text_field :name %>
    </p>
   
    <p>
      <%= form.label :email %><br>
      <%= form.text_area :email %>
    </p>
   
    <p>
      <%= form.submit %>
    </p>
  <% end %>   
  <%= link_to 'Voltar', patients_path %>