<!DOCTYPE html>

<html>

  <table class="table">
    <thead class="text-primary">
      <th>Nome</th>
      <th>Email</th>
      <th>Data de Nascimento</th>
      <th>Cidade</th>
    </thead>

    <tbody>
      <tr>
        <td><%= @patient.name %></td>
        <td><%= @patient.email %></td>
        <td><%= @patient.birth_date %></td>
        <td><%= @patient.city %></td>
      </tr>
    </tbody>
  </table>

  <%= link_to 'Voltar', patients_path %>
 
</html>
